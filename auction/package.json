{
  "name": "auction",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run front-release && npm run compile && node www/production.js",
    "dev": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --retain-lines",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile",
    "front-dev": "webpack-dev-server --inline -d --colors --display-error-details --display-cached --port 3000",
    "front-release": "webpack -p"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.7",
    "babel-runtime": "6.x.x",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "segment": "^0.1.3",
    "thinkjs": "^2.2.1",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-stage-1": "6.x.x",
    "json-loader": "^0.5.4",
    "less": "^2.3.1",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.4",
    "mongodb": "2.0.48",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "ts-loader": "0.8.1",
    "typescript": "next",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
}
